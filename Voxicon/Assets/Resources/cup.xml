<?xml version="1.0" encoding="us-ascii"?>
<VoxML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <Entity Type="Object" />
  <Lex>
    <Pred>cup</Pred>
    <Type>physobj*artifact</Type>
  </Lex>
  <Type>
    <Head>cylindroid[1]</Head>
    <Components>
      <Component Value="surface" />
      <Component Value="interior" />
    </Components>
    <Concavity>Concave</Concavity>
    <RotatSym>Y</RotatSym>
    <ReflSym>XY,YZ</ReflSym>
  </Type>
  <Habitat>
    <Intrinsic>
        <Intr Name="UP[2]" Value="align(Y,E_y)" />
        <Intr Name="TOP[2]" Value="top(+Y)" />
    </Intrinsic>
    <Extrinsic />
  </Habitat>
  <Afford_Str>
    <Affordances>
        <Affordance Formula="H[2]-&gt;[put(x, on([1]))]support([1], x)" />
        <Affordance Formula="H[2]-&gt;[put(x, in([1]))]contain([1], x)" />
        <Affordance Formula="H[2]-&gt;[grasp(x, [1])]" />
    </Affordances>
  </Afford_Str>
  <Embodiment>
    <Scale>&lt;agent</Scale>
    <Movable>true</Movable>
  </Embodiment>
</VoxML>